{
  "extends": "astro/tsconfigs/strict",
"include": [
  ".astro/types.d.ts",
  "types/global.d.ts",
  "src",
  "**/*"
],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}